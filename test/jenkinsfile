pipeline {
  agent {
    docker { image 'node:latest' }
  }
  stages {
    stage('Install') {
      steps { sh 'npm install' }
    }
 
    stage('Test') {
      parallel {
        stage('Static code analysis') {
            steps { echo 'corriendo Test' }
        }
        stage('Unit tests') {
            steps { sh 'npm run test' }
        }
      }
    }
 
    stage('Build') {
      steps { sh 'npm run build' }
    }
  }
}